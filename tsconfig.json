{
  "compileOnSave": false,
  "compilerOptions": {
    "importHelpers": true,
    "module": "esnext",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "plugins": [
      { "name": "tslint-language-service"}
    ],
    "baseUrl": "./src",
    "paths": {
      "@angular/*": [
        "../node_modules/@angular/*"
      ],
      "@nebular/*": [
        "../node_modules/@nebular/*"
      ],
      "@app/*": [
        "./app/*"
      ],
      "@dashboard": [
        "./app/dashboard"
      ],
      "@dashboard/*": [
        "./app/dashboard/*"
      ],
      "@appbooks": [
        "./app/books"
      ],
      "@appbooks/*": [
        "./app/books/*"
      ],
      "@appdata": [
        "./app/@core/data"
      ],
      "@appdata/*": [
        "./app/@core/data/*"
      ],
      "@apptheme": [
        "./app/@theme"
      ],
      "@apptheme/*": [
        "./app/@theme/*"
      ],
      "@apporders": [
        "./app/orders"
      ],
      "@apporders/*": [
        "./app/orders/*"
      ],
      "@appexchanges": [
        "./app/exchanges"
      ],
      "@appexchanges/*": [
        "./app/exchanges/*"
      ],
      "@appmerchandise": [
        "./app/merchandise"
      ],
      "@appmerchandise/*": [
        "./app/merchandise/*"
      ],
      "@apppartners": [
        "./app/partners"
      ],
      "@apppartners/*": [
        "./app/partners/*"
      ],
      "@appwarehouse": [
        "./app/warehouse"
      ],
      "@appwarehouse/*": [
        "./app/warehouse/*"
      ],
      "@appcodeMaster": [
        "./app/code-master"
      ],
      "@appcodeMaster/*": [
        "./app/code-master/*"
      ],
      "@appapplyfor": [
        "./app/apply-for"
      ],
      "@appapplyfor/*": [
        "./app/apply-for/*"
      ],
    }
  }
}
